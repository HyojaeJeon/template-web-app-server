{
  "name": "duri-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "npx @react-native-community/cli run-android",
    "ios": "npx @react-native-community/cli run-ios",
    "start": "npx @react-native-community/cli start --port 6002",
    "dev": "npm run start",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,md}\"",
    "test": "jest --config jest.config.js",
    "pod-install": "cd ios && pod install",
    "clean": "cd android && ./gradlew clean",
    "reset-cache": "npx @react-native-community/cli start --reset-cache",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@apollo/client": "^3.8.10",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "11.3.1",
    "@react-native-firebase/app": "^23.3.0",
    "@react-native-firebase/messaging": "^23.3.0",
    "@react-navigation/bottom-tabs": "7.4.7",
    "@react-navigation/native": "7.1.17",
    "@react-navigation/native-stack": "7.3.26",
    "@reduxjs/toolkit": "2.9.0",
    "apollo3-cache-persist": "^0.15.0",
    "date-fns": "^4.1.0",
    "graphql": "16.11.0",
    "i18next": "^23.11.5",
    "nativewind": "^4.1.23",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^14.1.2",
    "react-native": "0.80.0",
    "react-native-gesture-handler": "2.28.0",
    "react-native-reanimated": "^3.19.1",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "4.16.0",
    "react-native-svg": "^15.14.0",
    "react-redux": "9.2.0",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.7.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.25.3",
    "@react-native-community/cli": "20.0.0",
    "@react-native/babel-preset": "0.80.0",
    "@react-native/eslint-config": "0.80.0",
    "@react-native/metro-config": "0.80.0",
    "@testing-library/react-native": "^12.5.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "29.6.3",
    "patch-package": "^8.0.0",
    "prettier": "2.8.8",
    "react-native-clean-project": "^4.0.3"
  },
  "engines": {
    "node": ">=20"
  }
}
